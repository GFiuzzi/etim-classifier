# ETIM International API Configuration
# Copy this file to .env and fill in your actual credentials

# ETIM API OAuth2 Configuration
ETIM_AUTH_URL=https://etimauth.etim-international.com
ETIM_BASE_URL=https://etimapi.etim-international.com
ETIM_CLIENT_ID=your_client_id_here
ETIM_CLIENT_SECRET=your_client_secret_here
ETIM_SCOPE=EtimApi

# Server Configuration
PORT=3001
HOST=0.0.0.0
NODE_ENV=production

# Data Sources Priority (local, dataset, api)
DEFAULT_DATA_SOURCE=local